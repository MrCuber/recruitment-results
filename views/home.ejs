<style>
@import url('http://fonts.cdnfonts.com/css/neonblitz');
    @font-face {
   font-family: Neonblitz;
}
.tubeLight {
  color: #fff;
  text-shadow:
    0 0 7px #fff,
    0 0 10px #fff,
    0 0 21px #fff,
    0 0 42px #0fa,
    0 0 82px #0fa,
    0 0 92px #0fa,
    0 0 102px #0fa,
    0 0 151px #0fa;
}
.container {
    width: 100%;
}
.effect_div {
    margin-top: 15%;
}
body {
  font-size: 18px;
  font-family: "Neonblitz", sans-serif;
  background-color: #010a01;
}   

h1{
  text-align: center;
  font-weight: 400;
}   
  
h1 {
  font-size: 6.2rem;
  animation: pulsate 1.5s infinite alternate;  
  border: 0.2rem solid #fff;
  border-radius: 2rem;
  padding: 0.4em;
  box-shadow: 0 0 .2rem #fff,
            0 0 .2rem #fff,
            0 0 2rem #0fa,
            0 0 0.8rem #0fa,
            0 0 2.8rem #0fa,
            inset 0 0 1.3rem #0fa; 
}

@keyframes pulsate {
    
  100% {

      text-shadow:
      0 0 7px #fff,
    0 0 10px #fff,
    0 0 21px #fff,
    0 0 42px #0fa,
    0 0 82px #0fa,
    0 0 92px #0fa,
    0 0 102px #0fa,
    0 0 151px #0fa;
  
  }
  
  0% {

    text-shadow:
    0 0 7px #fff,
    0 0 10px #fff,
    0 0 21px #fff,
    0 0 42px #0fa,
    0 0 82px #0fa,
    0 0 92px #0fa,
    0 0 102px #0fa,
    0 0 151px #0fa;
}

}
@media only screen and (max-width: 1250px) {
  h1{
font-size:43px!important;
margin-top: 30%;
}
}
@media only screen and (max-width: 990px) {
  h1{
font-size:43px!important;
margin-top: 50%;
}
}
@media only screen and (max-width: 750px) {
